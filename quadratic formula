#include <iostream>
#include <cmath>
 
  using namespace std;
  
int main()
{
    int n;
    int c;
    int e;
    int a;
    int h;
    double sum2;
    double sum;
    
  cout << " Quadratic num ";
  cin >> n;
  c= -1;
  sum = n*c;
  cout << "the first b is "<< sum<< endl;
  
  // the first b in the quadratic formula 
  
  e = n*n;
  cout << " the second b is "<< e << endl;
  
  cout << "Put a and c";
  cin >> a;
  cin >> h;
  sum2 = a * 4 * h;
  cout << "the other answer is "<< sum2<< endl;
  
  
  return 0;
  
  
  
}
